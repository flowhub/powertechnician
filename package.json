    "url": "git+https://github.com/flowhub/powertechnician.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flowhub/powertechnician/issues"
  },
  "homepage": "https://github.com/flowhub/powertechnician#readme",
  "devDependencies": {
    "chai": "^4.0.2",
    "grunt": "^1.0.1",
    "grunt-mocha-test": "^0.13.2",
    "mocha": "^3.4.2",
    "msgflo": "^0.10.25",
    "noflo-nodejs": "^0.8.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "metar": "^1.0.0",
    "metar-taf": "0.0.2",
    "noflo": "^0.8.3",
