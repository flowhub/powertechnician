{
    "caseSensitive": false,
    "properties": {
        "name": "main",
        "environment": {
            "type": "noflo-nodejs"
        },
        "description": "Web server for the power technician",
        "icon": "globe"
    },
    "inports": {},
    "outports": {},
    "groups": [],
    "processes": {
        "xpress/Server_vp14o": {
            "component": "xpress/Server",
            "metadata": {
                "label": "Server",
                "x": 324,
                "y": 108,
                "width": 72,
                "height": 72
            }
        },
        "PortFromEnv": {
            "component": "core/ReadEnv",
            "metadata": {
                "label": "PortFromEnv",
                "x": 36,
                "y": 108,
                "width": 72,
                "height": 72
            }
        },
        "StaticServer": {
            "component": "powertechnician/ServeStatics",
            "metadata": {
                "label": "StaticServer",
                "x": 468,
                "y": 108,
                "width": 72,
                "height": 72
            }
        },
        "strings/SendString_r31ot": {
            "component": "strings/SendString",
            "metadata": {
                "label": "SendString",
                "x": 180,
                "y": 180,
                "width": 72,
                "height": 72
            }
        }
    },
    "connections": [
        {
            "src": {
                "process": "PortFromEnv",
                "port": "out"
            },
            "tgt": {
                "process": "xpress/Server_vp14o",
                "port": "port"
            },
            "metadata": {
                "route": 2
            }
        },
        {
            "src": {
                "process": "xpress/Server_vp14o",
                "port": "app"
            },
            "tgt": {
                "process": "StaticServer",
                "port": "in"
            },
            "metadata": {}
        },
        {
            "src": {
                "process": "PortFromEnv",
                "port": "error"
            },
            "tgt": {
                "process": "strings/SendString_r31ot",
                "port": "in"
            },
            "metadata": {
                "route": 1
            }
        },
        {
            "src": {
                "process": "strings/SendString_r31ot",
                "port": "out"
            },
            "tgt": {
                "process": "xpress/Server_vp14o",
                "port": "port"
            },
            "metadata": {
                "route": 2
            }
        },
        {
            "data": "VCAP_APP_PORT",
            "tgt": {
                "process": "PortFromEnv",
                "port": "key"
            }
        },
        {
            "data": "/static",
            "tgt": {
                "process": "StaticServer",
                "port": "prefix"
            }
        },
        {
            "data": "static",
            "tgt": {
                "process": "StaticServer",
                "port": "path"
            }
        },
        {
            "data": "5000",
            "tgt": {
                "process": "strings/SendString_r31ot",
                "port": "string"
            }
        }
    ]
}